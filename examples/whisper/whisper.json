{
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_script": "code/user_script.py",
            "script_dir": "code",
            "hf_config": {
                "task" : "automatic-speech-recognition",
                "model_name" : "openai/whisper-tiny.en"
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    "cpu"
                ]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [
                        {
                            "name": "avg"                        
                        }
                    ],
                    "user_config": {
                        "user_script": "code/user_script.py",
                        "script_dir": "code",
                        "data_dir": "data",
                        "dataloader_func": "whisper_no_audio_decoder_dataloader"
                    }
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "EnDeConversion",
            "config": {
                "target_opset": 17
            }
        },
        "onnx_dynamic_quantization": {
            "type": "OnnxDynamicQuantization",
            "disable_search": true,
            "config": {
                "per_channel": false,
                "reduce_range": false,
                "op_types_to_quantize": [
                    "MatMul",
                    "Gemm",
                    "Gather"
                ],
                "MatMulConstBOnly": false
            }
        },
        "insert_beam_search": {
            "type": "InsertBeamSearch"
        },
        "prepost": {
            "type": "AppendPrePostProcessingOps",
            "config": {
                "tool_command": "whisper",
                "testdata_filepath": "data/1272-141231-0002.mp3",
                "tool_command_args": {
                    "model_name": "openai/whisper-tiny.en",
                    "use_audio_decoder": false
                }
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "search_strategy": false,
        "evaluator": "common_evaluator",
        "host": "local_system",
        "target": "local_system",
        "clean_cache": true,
        "cache_dir": "cache",
        "output_dir": "models",
        "output_name": "whisper"
    }
}